	for i,0,SKILL_MAX
		if strlen(skill_sinfo(i,SKILL_SINFO_NAME)) <= 0 : _continue
		; RANKを自動計算
		; スキルのランク
		;	消費AP	ランク
		;	0		1
		;	〜10	2
		;	〜20	3
		;	〜30	4
		;	〜40	5
		;	41〜	6
		;	100〜	7
		if skill_info(i,SKILL_INFO_USEAP) > 0 {
			if skill_info(i,SKILL_INFO_USEAP) > 10 {
				if skill_info(i,SKILL_INFO_USEAP) > 20 {
					if skill_info(i,SKILL_INFO_USEAP) > 30 {
						if skill_info(i,SKILL_INFO_USEAP) > 40 {
							if skill_info(i,SKILL_INFO_USEAP) >= 100 {
									skill_info(i,SKILL_INFO_RANK) = 7
							} else {
								skill_info(i,SKILL_INFO_RANK) = 6
							}									
						} else {
							skill_info(i,SKILL_INFO_RANK) = 5
						}					
					} else {
						skill_info(i,SKILL_INFO_RANK) = 4
					}		
				} else {
					skill_info(i,SKILL_INFO_RANK) = 3
				}
			} else {
				skill_info(i,SKILL_INFO_RANK) = 2
			}
		} else {
			skill_info(i,SKILL_INFO_RANK) = 1
		}
		
		; レアでもユニークでもないスキルにノーマルスキルを付与する
		if ( ( skill_info(i,SKILL_INFO_FLAG) & SKILL_INFO_FLAG_UNIQUE ) == 0 ) and ( ( skill_info(i,SKILL_INFO_FLAG) & SKILL_INFO_FLAG_RARE ) == 0 ) {
			skill_info(i,SKILL_INFO_FLAG) |= SKILL_INFO_FLAG_NORMAL
		}
		
		; ランダムテーブルにスキルを追加する
		sub_add_random_table_skill i
	next

